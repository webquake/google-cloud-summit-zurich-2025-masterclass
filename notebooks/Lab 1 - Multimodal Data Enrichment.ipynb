        "      URI: ''', posters.uri) AS prompt\n",
        "      , posters.uri\n",
        "      FROM `masterclass.movie_posters` AS posters\n",
        "    ),\n",
        "    STRUCT(\n",
        "      0.2 AS temperature,\n",
        "      1024 AS max_output_tokens,\n",
        "      TRUE AS flatten_json_output\n",
        "    )\n",
        "  );"
      ],
      "metadata": {
        "id": "XNxrOq9UJRGi"
      },
